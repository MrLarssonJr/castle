module DatabaseSecret extends "@easy-k8s/Secret.pkl"

username: String?
password: String?

name = "database"
fixed username_key = "username"
fixed password_key = "password"
data {
  when (!(username is Null)) {
    [username_key] = username
  }
  when (!(password is Null)) {
    [password_key] = password
  }
}
